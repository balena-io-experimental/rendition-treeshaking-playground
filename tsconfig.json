{
	"compilerOptions": {
		"module": "es2015",
		"moduleResolution": "node",
		"target": "es2015",
		"outDir": "build",
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"declaration": true,
		"skipLibCheck": true,
	},
	"include": [
		"src/*.ts"
	]
}
